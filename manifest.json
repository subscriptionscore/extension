{
  "name": "Subscription Score",
  "description": "Never regret subscribing to a mailing list again",
  "version": "0.3.3",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "assets/logo.png"
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "web_accessible_resources": ["frame.html", "frame.bundle.js", "frame.css"],
  "permissions": ["<all_urls>", "tabs", "storage"],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*/*"],
      "js": ["/inboxsdk.js", "/gmail.bundle.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["<all_urls>"],
      "js": ["/content.bundle.js"],
      "run_at": "document_idle"
    }
  ]
}
